{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "^/_server-islands/([^/]+?)/?$",
      "dest": "_render"
    },
    {
      "src": "^/_image/?$",
      "dest": "_render"
    },
    {
      "src": "^/api/example/?$",
      "dest": "_render"
    },
    {
      "src": "^/api/stamp/?$",
      "dest": "_render"
    },
    {
      "src": "^/game/?$",
      "dest": "_render"
    },
    {
      "src": "^/$",
      "dest": "_render"
    }
  ]
}
